      <TextField fx:id="txtfTitle" layoutX="24.0" layoutY="45.0" prefHeight="34.0" prefWidth="202.0" promptText="Title..." />
      <TextField fx:id="txtfIMDBRating" layoutX="24.0" layoutY="85.0" prefHeight="34.0" prefWidth="202.0" promptText="IMDB-Rating..." />
      <TextField fx:id="txtfPersonalRating" layoutX="24.0" layoutY="125.0" prefHeight="34.0" prefWidth="202.0" promptText="Personal-Rating..." />
      <TextField fx:id="txtfYear" layoutX="24.0" layoutY="165.0" prefHeight="34.0" prefWidth="202.0" promptText="Year..." />
